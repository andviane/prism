<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.us.andriod"
    android:versionCode="5"
    android:versionName="1.5"
    android:installLocation="auto"
    android:hardwareAccelerated="false"
 >

 <uses-sdk android:minSdkVersion="10"
              android:targetSdkVersion="10" />

    <application
        android:icon="@drawable/ic_launcher"
        android:installLocation="auto"
        android:allowBackup="true"
        android:label="@string/app_name" >
        <activity
            android:label="@string/app_name"
            android:name=".An_StartActivity" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="Prism" android:name="PrismActivity"></activity>
        <activity android:label="Other" android:name="OtherActivity"></activity>
        <activity android:label="Microscope" android:name="MicroscopeActivity"></activity>
        <activity android:label="Telescope" android:name="TelescopeActivity"></activity>                
        <activity android:label="Documentation" android:name="ViewActivity"></activity>
    </application>

</manifest>
